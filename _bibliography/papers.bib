	html = {http://link.springer.com/10.1140/epjb/e2014-40699-x},
	issn = {1434-6028},
	journal = {The European Physical Journal B},
	month = feb,
	number = {2},
	pages = {27},
	title = {Quantitative comparison between crowd models for evacuation planning and evaluation},
	url = {http://link.springer.com/10.1140/epjb/e2014-40699-x},
	volume = {87},
	year = {2014},
	bdsk-url-1 = {http://link.springer.com/10.1140/epjb/e2014-40699-x},
