	publisher = {Public Library of Science},
	title = {{Evaluating temporal factors in combined interventions of workforce shift and school closure for mitigating the spread of influenza.}},
	url = {http://dx.plos.org/10.1371/journal.pone.0032203 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3293885\&tool=pmcentrez\&rendertype=abstract},
