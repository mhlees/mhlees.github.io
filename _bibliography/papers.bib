	author = {Lees, Michael and Logan, Brian and Theodoropoulos, G.K.},
	month = aug,
	year = {2006},
	pages = {752--767},
}

@inproceedings{lees_analysing_2006-1,
	title = {Analysing the {Performance} of {Optimistic} {Synchronisation} {Algorithms} in {Simulations} of {Multi}-{Agent} {Systems}},
