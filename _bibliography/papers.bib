@inproceedings{lees_decision-theoretic_2005,
	title = {Decision-{Theoretic} {Throttling} for {Optimistic} {Simulations} of {Multi}-{Agent} {Systems}},
	isbn = {0-7695-2462-1},
	url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=1530667},
