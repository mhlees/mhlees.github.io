	publisher = {IEEE},
	title = {{An Adaptive Load Management Mechanism for Distributed Simulation of Multi-agent Systems}},
	url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=1530668},
	year = {2005},
