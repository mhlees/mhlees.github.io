	keywords = {Communicable Disease Control,Communicable Disease Control: methods,Community-Acquired Infections,Community-Acquired Infections: prevention \& contro,Computer Simulation,Disease Transmission,Health Policy,Human,Human: prevention \& control,Human: transmission,Humans,Infectious,Infectious: prevention \& control,Influenza,Neural Networks (Computer),Schools,Singapore,Time Factors},
