	pages = {72--80},
	publisher = {IEEE},
	title = {{A Computational Model of Emotions for Agent-Based Crowds in Serious Games}},
	url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=6040757},
	volume = {2},
